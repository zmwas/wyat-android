<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wyat.wyat">

    <application
        android:name=".application.WyatApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".accounts.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />


            </intent-filter>
        </activity>
        <activity android:name=".accounts.activities.SignUpActivity"/>
        <activity android:name=".events.activities.EventListActivity"/>
        <activity android:name=".events.activities.PostEventActivity"/>
        <activity android:name=".events.activities.EventDetailActivity"/>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


    </application>


    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>